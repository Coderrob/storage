   <ItemGroup>
      <PackageReference Include="Microsoft.Azure.Storage.File" Version="11.1.0" />
   </ItemGroup>

   <ItemGroup>
     <ProjectReference Include="..\..\Storage.Net\Storage.Net.csproj" />
   </ItemGroup>

</Project>
